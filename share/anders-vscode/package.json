{
    "name": "infinity",
    "displayName": "infinity",
    "version": "0.0.1",
    "description": "Groupoid Infinity syntax highlighting for Visual Studio Code",
    "publisher": "Groupoid",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/groupoid/axio/"
    },
    "engines": {
        "vscode": "^0.10.10"
    },
    "contributes": {
        "languages": [{
            "id": "infinity",
            "extensions": [
                ".henk",
                ".frank",
                ".christine",
                ".per",
                ".jack",
                ".fabien",
                ".urs",
                ".dan",
                ".leslie",
                ".laurent",
                ".anders"
            ],
            "configuration": "./infinity-configuration.json"
        }],
        "grammars": [{
                "language": "infinity",
                "scopeName": "sources.infinity",
                "path": "./syntaxes/infinity.tmLanguage.json"
        }]
    }
}
