{
    "name": "anders",
    "fileTypes": [
        "anders"
    ],
    "patterns": [
        {
            "begin": "{-",
            "end": "-}",
            "name": "comment.block.anders"
        },
        {
            "match": "--.*$",
            "name": "comment.line.double-dash.anders"
        },
        {
            "match": "(?<=^|[\\s()\\[\\]])(option|axiom|postulate|def|definition|theorem|lemma|corollary|proposition|import|with|module|where|begin|end)(?=$|[\\s()\\[\\]])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s(),\\[\\]])(λ|coequ|ι|resp|coequ-ind|hs|hs-ind|center|hub|spoke|Σ|Π|Id|W|ℑ|ℑ-unit|ind-ℑ|ℑ-join|indᵂ|ind₀|ind₁|ind₂|sup|ref|idJ|forall|summa|Partial|PartialP|inc|ouc|PathP|hcomp|transp|Glue|glue|unglue)(?=$|[\\s(),\\[\\]])",
            "name": "keyword.control.anders"
        },
        {
            "match": "(?<=^|[\\s()\\[\\],])([UV][₀₁₂₃₄₅₆₇₈₉]*)(?=$|[\\s()\\[\\],])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()\\[\\]])(->|→|↦|\\|->|:=|≔|≜|≝|:|\\*|\\\\/|/\\\\|∧|∨|@)(?=$|[\\s()\\[\\]])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()\\[\\]])(-)",
            "name": "keyword.other.anders"
        },
        {
            "match": "[><]",
            "name": "keyword.other.anders"
        }
    ],
    "scopeName": "source.anders"
}
